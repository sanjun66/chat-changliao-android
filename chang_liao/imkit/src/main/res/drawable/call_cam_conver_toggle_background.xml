<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/background"
        android:drawable="@drawable/call_toggle_button_background" />
    <item
        android:id="@+id/toggle">
        <selector>
            <item android:drawable="@drawable/ic_camera_reverse" android:state_checked="true" />
            <item android:drawable="@drawable/ic_camera_reverse" android:state_checked="false" />
        </selector>
    </item>

</layer-list>